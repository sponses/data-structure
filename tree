class Node{
  constructor(key){
    this.key = key;
    this.left = null;
    this.right = null;
  }
}

class BinarySearchTree{
  constructor(){
    this.root = null;
  }
  
  insert(key){
    const newNode = new Node(key);
    const insertNode = (node,newNode){
      if(newNode.key < node.key){
        if(node.left === null){
          node.left = nodeNode;
        }else{
          insertNode(node.left,newNode);
        }
      }else{
        if(node.right === null){
          node.right = newNode;
        }else{
          insertNode(node.right,newNode)
        }
      }
      if(!this.root){
        this.root = newNode;
       }else{
        insertNode(this.root,newNode);
       }
   }
}
