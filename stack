class stack {
  constructor(){
    this.stack = [];
  }
  
  //入栈
  push(e){
    this.stack.push(e)
  }
  
  //出栈
  pop(){
    this.stack.pop();
  }
  
  //获取栈顶元素
  getTop(){
    return this.stack[this.stack.length-1];
  }
  
  //是否为空栈
  isEmpty(){
    return !!this.stack.length;
  }
  
  //获取栈的尺寸
  getSize(){
    return this.stack.length;
  }
  
  //清空栈
  clear(){
    this.stack = [];
  }
  
  //输出栈
  print(){
    console.log(this.stack);
  }
  
}
